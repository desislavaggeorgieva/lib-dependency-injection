{
    "name": "paysera/lib-dependency-injection",
    "description": "Adds helper classes to Symfony DependencyInjection component",
    "autoload": {
        "psr-4": {
            "Paysera\\Component\\DependencyInjection\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Paysera\\Component\\DependencyInjection\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.3.3",
        "symfony/dependency-injection": "2.*|3.*",
        "symfony/config": "2.*|3.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7"
    }
}
